  "dependencies": {
    "debug": "^2.6.9",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.72",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.10.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.5"
  }
}
